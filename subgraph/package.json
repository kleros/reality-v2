{
  "name": "@kleros/reality-v2-subgraph",
  "version": "2.0.0",
  "license": "MIT",
  "scripts": {
    "clean": "graph clean",
    "codegen": "graph codegen",
    "build": "graph build",
    "create-local": "graph create --node http://localhost:8020/ kleros/reality-v2-local",
    "remove-local": "graph remove --node http://localhost:8020/ kleros/reality-v2-local",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 kleros/reality-v2-local --version-label v$(date +%s)",
    "rebuild-deploy-local": "yarn clean && yarn codegen && yarn build && yarn create-local && yarn deploy-local",
    "start-local-indexer": "docker compose -f ../services/graph-node/docker-compose.yml up -d && docker compose -f ../services/graph-node/docker-compose.yml logs -f",
    "stop-local-indexer": "docker compose -f ../services/graph-node/docker-compose.yml down && rm -rf ../services/graph-node/data"
  },
  "packageManager": "yarn@4.0.2+sha256.825003a0f561ad09a3b1ac4a3b3ea6207af2796d54f62a9420520915721f5186",
  "volta": {
    "node": "20.11.0"
  },
  "dependencies": {
    "@graphprotocol/graph-ts": "^0.32.0"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "0.64.0",
    "@kleros/reality-v2-eslint-config": "workspace:^",
    "@kleros/reality-v2-prettier-config": "workspace:^",
    "gluegun": "^5.1.2",
    "matchstick-as": "0.6.0"
  },
  "dependenciesComments": {
    "@graphprotocol/graph-cli": "pinned because of this issue: https://github.com/graphprotocol/graph-tooling/issues/1399#issuecomment-1676104540"
  }
}
